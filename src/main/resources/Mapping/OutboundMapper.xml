  <select id="queryList" resultMap="BaseResultMap"  parameterType="Object">
    select *
    from outbound a
    <include refid="Example_Where_Clause"/>
    and a.IS_DELETE = 0
    order by a.ID desc
    <if test="pageSize != null and pageSize != ''" >
